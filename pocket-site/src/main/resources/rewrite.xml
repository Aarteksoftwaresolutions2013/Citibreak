<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <!-- Sites & Events details -->
    <rule>
        <from>^/(sites|events)/[^/]+/details/([a-z0-9]+)/$</from>
        <to>/$1/$2/details/</to>
    </rule>

</urlrewrite>